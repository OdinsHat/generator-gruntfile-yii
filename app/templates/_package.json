{
  "name": "<%= _.slugify(appname) %>",
  "version": "<%= version %>",
  "dependencies": {},
  "devDependencies": {
    "grunt": "^0.4.5",
    "grunt-php": "^0.3.3",
    "grunt-phplint": "0.0.5",
    <% if (phpCs) { %>
    "grunt-phpcs": "^0.2.2",
    <% } %>
    <% if (phpMd) { %>
    "grunt-phpmd": "^0.1.0",
    <% } %>
    <% if (phpCpd) { %>
    "grunt-phpcpd": "^0.1.3",
    <% } %>
    <% if (phpCsFixer) { %>
    "grunt-php-cs-fixer": "0.0.5",
    <% } %>
    <% if (jsLint) { %>
    "grunt-contrib-jshint": "^0.10.0",
    "jshint-stylish": "^0.2.0"
    <% } %>
  }
}
